update student_on_course_kn t
set student_rating = CASE WHEN (0<student_rating and student_rating<60) THEN  student_rating-5
WHEN (61<student_rating and student_rating<80) THEN  student_rating-10
WHEN (81<student_rating and student_rating<90) THEN  student_rating-15
ELSE student_rating-20 end;
SELECT * FROM student_on_course_kn ORDER BY id; 
