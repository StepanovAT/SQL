SELECT n.name as student_name, cu.name as course_name ,co.name as student_college ,st.student_rating
from student n, course cu, student_on_course st, college co
WHERE  n.id = st.student_id and st.course_id = cu.id and st.student_rating>50 and co.size>5000 and st.student_id = co.id
order by n.name, cu.name;
